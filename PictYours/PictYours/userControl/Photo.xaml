             xmlns:local="clr-namespace:PictYours.userControl" xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <materialDesign:Card Margin="0,0,0,2">
            
        </materialDesign:Card>
        <Grid Background="{StaticResource MaterialDesignDarkBackground}" Grid.Row="1">
            
        </Grid>
    </Grid>
</UserControl>
